{
  "name": "the-scheduler-api",
  "version": "1.0.0",
  "description": "api for the-scheduler frontend",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-resolvers": "2.7.2",
    "@types/auth0": "^2.35.3",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.6.1",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@prisma/client": "^4.1.1",
    "apollo-server": "^3.10.0",
    "auth0": "^2.42.0",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.21",
    "twilio": "^3.83.0"
  },
  "engines": {
    "node": "18.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "dev": "ts-node-dev --transpile-only --no-notify --exit-child src/server.ts",
    "generate": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:deploy:test": "dotenv -e .env.test -- yarn prisma:deploy",
    "postinstall": "yarn prisma:generate && yarn generate",
    "seed:parse": "ts-node-dev --transpile-only --no-notify --exit-child prisma/parseData.ts",
    "seed": "ts-node-dev --transpile-only --no-notify --exit-child prisma/seedTables.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "jest:test": "dotenv -e .env.test -- jest --runInBand",
    "test": "yarn docker:up && yarn prisma:deploy:test && yarn jest:test && yarn docker:down"
  }
}
